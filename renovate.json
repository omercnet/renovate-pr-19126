{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "prCreation": "immediate",
  "dependencyDashboard": true,
  "dependencyDashboardApproval": false,
  "digest": {
    "enabled": true
  },
  "schedule": ["at any time"],
  "packageRules": [
    {
      "digest": {
        "enabled": true
      },
      "groupName": "github",
      "matchPackagePrefixes": ["github.com/omercnet"]
    }
  ]
}
